<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/nav_viewpager">

    <fragment
        android:id="@+id/nav_viewpager"
        android:name="com.example.android_task.ViewPager"
        android:label="ViewPager"
        tools:layout="@layout/fragment_view_pager"/>
    <activity
        android:id="@+id/nav_music"
        android:name="com.example.android_task.MusicMainActivity"
        android:label="activity_music_main"
        tools:layout="@layout/activity_music_main" />
    <fragment
        android:id="@+id/nav_news"
        android:name="com.example.android_task.NewsFragment"
        android:label="fragment_news"
        tools:layout="@layout/fragment_news" />
    <fragment
        android:id="@+id/nav_circle"
        android:name="com.example.android_task.CustomCircleFragment"
        android:label="fragment_circle"
        tools:layout="@layout/fragment_custom_circle" />
    <activity
        android:id="@+id/nav_map"
        android:name="com.example.android_task.GoogleMapsActivity"
        android:label="activity_google_maps"
        tools:layout="@layout/activity_google_maps" />
</navigation>
